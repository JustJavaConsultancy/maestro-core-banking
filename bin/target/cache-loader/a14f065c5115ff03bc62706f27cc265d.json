I,CAAC,oBAAoB,CAAC,CAAC;wBACrC,CAAC,EACD,SAAS,EAAC,qBAAqB,EAC/B,GAAG,EAAE,KAAK;wBAEV,gCAAK,KAAK,GAAG,CAAC,GAAG,MAAM,CAAC,IAAI,CAAC,GAAG,IAAI,CAAM;wBAC1C,gCAAK,IAAI,CAAC,WAAW,CAAM;wBAC3B,gCAAK,IAAI,CAAC,IAAI,CAAC,KAAK,CAAM;wBAC1B,gCAAK,IAAI,CAAC,IAAI,CAAC,SAAS,CAAM;wBAC9B,gCAAK,IAAI,CAAC,IAAI,CAAC,QAAQ,CAAM;wBAC7B,gCAAK,IAAI,CAAC,SAAS,CAAC,MAAM,GAAG,CAAC,CAAC,CAAC,CAAC,IAAI,CAAC,SAAS,CAAC,CAAC,CAAC,CAAC,YAAY,CAAC,GAAG,CAAC,QAAQ,CAAC,CAAC,CAAC,CAAC,CAAM;wBACtF,gCAAK,IAAI,CAAC,SAAS,CAAC,MAAM,GAAG,CAAC,CAAC,CAAC,CAAC,IAAI,CAAC,SAAS,CAAC,CAAC,CAAC,CAAC,OAAO,CAAC,CAAC,CAAC,EAAE,CAAM;wBACrE,gCAAK,MAAM,CAAC,IAAI,CAAC,WAAW,CAAC,CAAC,MAAM,CAAC,IAAI,CAAC,CAAM;wBAChD;4BACE,6BACE,KAAK,EAAE;oCACL,OAAO,EAAE,UAAU;oCACnB,YAAY,EAAE,MAAM;oCACpB,SAAS,EAAE,QAAQ;oCACnB,QAAQ,EAAE,MAAM;oCAChB,KAAK,EAAE,SAAS;oCAChB,eAAe,EAAE,yBAAyB;iCAC3C,IAEA,IAAI,CAAC,IAAI,CAAC,SAAS,CAAC,CAAC,CAAC,QAAQ,CAAC,CAAC,CAAC,UAAU,CACxC,CACH,CACF,CACN,CACJ,CACK,CACF,CACJ,CACM,CACP,CACV,CAAC;AACJ,CAAC;AAED,MAAM,eAAe,GAAG,CAAC,EAAE,OAAO,EAAE,cAAc,EAAE,EAAE,eAAe,EAAE,OAAO,EAAE,qBAAqB,EAAE,EAAc,EAAE,EAAE,CAAC,CAAC;IACzH,IAAI,EAAE,OAAO,CAAC,IAAI;IAClB,OAAO,EAAE,OAAO;CACjB,CAAC,CAAC;AAEH,MAAM,kBAAkB,GAAG,EAAE,OAAO,EAAE,CAAC;AAKvC,eAAe,OAAO,CAAC,eAAe,EAAE,kBAAkB,CAAC,CAAC,KAAK,CAAC,CAAC","sourcesContent":["import Axios from 'axios';\r\nimport React, { useEffect, useState, useRef } from 'react';\r\nimport InnerLayout from '../components/Layout';\r\nimport Layout from '../Layout';\r\nimport { Table } from 'react-bootstrap';\r\nimport { setUser } from 'app/shared/reducers/reports';\r\nimport { RouteComponentProps } from 'react-router-dom';\r\nimport { IRootState } from 'app/shared/reducers';\r\nimport { connect } from 'react-redux';\r\nimport { useHistory } from 'react-router-dom';\r\nimport moment from 'moment';\r\nimport { isEmpty } from 'lodash';\r\n\r\nimport { headers } from './data/customerData';\r\n\r\nexport interface IUserstateProps extends StateProps, DispatchProps, RouteComponentProps<{}> {}\r\nfunction Users(props) {\r\n  const [users, setUsers] = useState([]);\r\n  const [total, setTotal] = useState(0);\r\n  const [size, setSize] = useState(20);\r\n  const [page, setPage] = useState(0);\r\n  const [search, setSearch] = useState('');\r\n  const history = useHistory();\r\n  const toPrint = useRef();\r\n  const [schemes, setSchemes] = useState([]);\r\n  const [scheme, setScheme] = useState({ schemeID: '' });\r\n\r\n  const [isDisable, setIsDisable] = useState(true);\r\n\r\n  const { schemeID } = scheme;\r\n\r\n  const getUsers: any = async (paget, schem) => {\r\n    try {\r\n      const theUsers = await Axios.get(`/api/profiles/${schem}/search?key=${search}&page=${paget}&size=${size}`);\r\n      console.error(theUsers);\r\n      setTotal(theUsers.data.metadata.totalNumberOfRecords);\r\n      setPage(paget);\r\n      setUsers(theUsers.data.data);\r\n\r\n      // eslint-disable-next-line no-console\r\n      console.log(theUsers);\r\n      // for(let x = 0; x <=)\r\n    } catch (e) {\r\n      console.error(e);\r\n    }\r\n  };\r\n\r\n  const searchUsers: any = async (s, schem) => {\r\n    try {\r\n      const theUsers = await Axios.get(`/api/profiles/${schem}/search?key=${s}&page=${0}&size=${size}`);\r\n      console.error(theUsers);\r\n      setTotal(theUsers.data.metadata.totalNumberOfRecords);\r\n      setPage(0);\r\n      setUsers(theUsers.data.data);\r\n      // for(let x = 0; x <=)\r\n    } catch (e) {\r\n      console.error(e);\r\n    }\r\n  };\r\n\r\n  const getSchemes = async () => {\r\n    try {\r\n      const theSchemes = await Axios.get(`api/schemes/admin/${props.userLog.login}`);\r\n      console.error(theSchemes);\r\n      setSchemes(theSchemes.data.data);\r\n      setScheme(theSchemes.data.data[0]);\r\n      getUsers(0, theSchemes.data.data[0].schemeID);\r\n    } catch (e) {\r\n      console.error(e);\r\n    }\r\n  };\r\n\r\n  const handleschemeChange = s => {\r\n    console.error(s.schemeID);\r\n\r\n    setScheme(s);\r\n    getUsers(0, s.schemeID);\r\n  };\r\n\r\n  const asyncExportMethod = async () => {\r\n    try {\r\n      const result = await Axios.get(`/api/profiles/${scheme.schemeID}/search?key=${search}&page=${0}&size=${1000}`);\r\n      return result;\r\n    } catch (error) {\r\n      console.error(error);\r\n      return [];\r\n    }\r\n  };\r\n\r\n  useEffect(() => {\r\n    getSchemes();\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    if (scheme.schemeID) {\r\n      setIsDisable(false);\r\n    } else {\r\n      setIsDisable(true);\r\n      // console.error(\"specs\")\r\n    }\r\n  }, [schemeID]);\r\n\r\n  return (\r\n    <Layout>\r\n      <InnerLayout\r\n        toPrint={toPrint}\r\n        toExport={users}\r\n        schemes={schemes}\r\n        schemeChange={s => {\r\n          handleschemeChange(JSON.parse(s));\r\n        }}\r\n        setSearch={s => {\r\n          setSearch(s);\r\n          searchUsers(s, scheme.schemeID);\r\n        }}\r\n        nextPage={pageto => getUsers(pageto, scheme.schemeID)}\r\n        page={total}\r\n        size={size}\r\n        getData={asyncExportMethod}\r\n        isDisable={isDisable}\r\n        headers={headers}\r\n        title=\"Customer Details\"\r\n        path=\"Customer\"\r\n      >\r\n        <div ref={toPrint} style={{ minWidth: '300px', minHeight: '300px', marginTop: '20px' }}>\r\n          <Table hover responsive className=\"transaction-table\">\r\n            <thead className=\"tableListData\">\r\n              <th>S/N</th>\r\n              <th>Customer ID</th>\r\n              <th>Email</th>\r\n              <th>First Name</th>\r\n              <th>Surname</th>\r\n              <th>KYC Level</th>\r\n              <th>Address</th>\r\n              <th>Date Registered</th>\r\n              <th>Status</th>\r\n            </thead>\r\n            <tbody>\r\n              {users.map(\r\n                (user, index) =>\r\n                  !isEmpty(user) && (\r\n                    <tr\r\n                      onClick={() => {\r\n                        props.setUser(user);\r\n                        history.push('/customers-details');\r\n                      }}\r\n                      className=\"tableListData click\"\r\n                      key={index}\r\n                    >\r\n                      <td>{index + 1 + Number(page) * size}</td>\r\n                      <td>{user.phoneNumber}</td>\r\n                      <td>{user.user.email}</td>\r\n                      <td>{user.user.firstName}</td>\r\n                      <td>{user.user.lastName}</td>\r\n                      <td>{user.addresses.length > 0 ? user.addresses[0].addressOwner.kyc.kycLevel : 1}</td>\r\n                      <td>{user.addresses.length > 0 ? user.addresses[0].address : ''}</td>\r\n                      <td>{moment(user.dateCreated).format('ll')}</td>\r\n                      <td>\r\n                        <div\r\n                          style={{\r\n                            padding: '5px 10px',\r\n                            borderRadius: '15px',\r\n                            textAlign: 'center',\r\n                            fontSize: '12px',\r\n                            color: '#00A07E',\r\n                            backgroundColor: 'rgba(0, 196, 140, 0.27)',\r\n                          }}\r\n                        >\r\n                          {user.user.activated ? 'Active' : 'inActive'}\r\n                        </div>\r\n                      </td>\r\n                    </tr>\r\n                  )\r\n              )}\r\n            </tbody>\r\n          </Table>\r\n        </div>\r\n      </InnerLayout>\r\n    </Layout>\r\n  );\r\n}\r\n\r\nconst mapStateToProps = ({ reports, authentication: { isAuthenticated, account, sessionHasBeenFetched } }: IRootState) => ({\r\n  user: reports.user,\r\n  userLog: account,\r\n});\r\n\r\nconst mapDispatchToProps = { setUser };\r\n\r\ntype StateProps = ReturnType<typeof mapStateToProps>;\r\ntype DispatchProps = typeof mapDispatchToProps;\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Users);\r\n"]}]}