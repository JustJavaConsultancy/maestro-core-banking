ACV,KAAK,EAAE,MAAM,CAAC,CAAC,CAAC,MAAM,CAAC,EACvB,WAAW,EAAC,MAAM,EAClB,iBAAiB,EAAE,IAAI,EACvB,YAAY,EAAE,CAAC,EACf,iBAAiB,EAAE,IAAI,GAEvB,CACC;4BACL;gCACE,oBAAC,WAAW,IACV,KAAK,EAAE,MAAM,CAAC,CAAC,CAAC,cAAc,CAAC,EAC/B,WAAW,EAAC,MAAM,EAClB,iBAAiB,EAAE,IAAI,EACvB,YAAY,EAAE,CAAC,EACf,iBAAiB,EAAE,IAAI,GAEvB,CACC,CACF,CACN,CAAC;wBACF;4BACE,+BAAS;4BACT,+BAAS;4BACT,+BAAS;4BACT,+BAAS;4BACT,4BAAI,KAAK,EAAE,EAAE,QAAQ,EAAE,MAAM,EAAE,sBAAsB;4BACrD,+BAAS;4BACT,+BAAS;4BACT,4BAAI,KAAK,EAAE,EAAE,QAAQ,EAAE,MAAM,EAAE,IAE3B,oBAAC,WAAW,IACV,KAAK,EAAE,OAAO,EACd,WAAW,EAAC,MAAM,EAClB,iBAAiB,EAAE,IAAI,EACvB,YAAY,EAAE,CAAC,EACf,iBAAiB,EAAE,IAAI,GAEvB,CAED,CACF,CACC,CACF,CACJ,CACE,CACT,CACJ,CAAC;AACJ,CAAC;AAED,MAAM,eAAe,GAAG,CAAC,EAAE,OAAO,EAAc,EAAE,EAAE,CAAC,CAAC;IACpD,IAAI,EAAE,OAAO,CAAC,IAAI;CACnB,CAAC,CAAC;AAEH,MAAM,kBAAkB,GAAG,EAAE,aAAa,EAAE,CAAC;AAK7C,eAAe,OAAO,CAAC,eAAe,EAAE,kBAAkB,CAAC,CAAC,MAAM,CAAC,CAAC","sourcesContent":["import '../wallet.scss';\r\nimport React, { useState, useEffect, useRef } from 'react';\r\nimport { Table } from 'reactstrap';\r\nimport Axios from 'axios';\r\nimport { connect } from 'react-redux';\r\nimport { IRootState } from 'app/shared/reducers';\r\nimport { RouteComponentProps } from 'react-router-dom';\r\nimport moment from 'moment';\r\nimport { setWalletData } from 'app/shared/reducers/reports';\r\nimport ReactToPrint from 'react-to-print';\r\nimport { isEmpty } from 'lodash';\r\nimport exportFromJSON from 'export-from-json';\r\nimport MoneyFormat from 'react-number-format';\r\nimport processData, { xcelData } from './ProcessDataToExport';\r\n\r\nexport interface IStatementProps extends StateProps, DispatchProps, RouteComponentProps<{}> {}\r\nfunction Wallet(props: IStatementProps) {\r\n  const [statement, setStatement] = useState([\r\n    { transactionRef: '', currentBalance: '', credit: false, debit: false, memo: '', transactionDate: '', externalRef: '' },\r\n  ]);\r\n  const [opening, setOpening] = useState('');\r\n  const [closing, setClosing] = useState('');\r\n  const [data, setData] = useState({\r\n    accountOwnerName: '',\r\n    dateOpened: moment(Date()).format('YYYY-MM-DD'),\r\n    accountNumber: '',\r\n    schemeName: '',\r\n    start: '',\r\n    end: '',\r\n  });\r\n  const [format, setFormat] = useState('pdf');\r\n  const toPrint = useRef();\r\n\r\n  const exportFile = async () => {\r\n    enum toexport {\r\n      csv = 'csv',\r\n      xls = 'csv',\r\n      txt = 'txt',\r\n      json = 'json',\r\n    }\r\n    let dataToExport: xcelData[];\r\n    try {\r\n      dataToExport = await processData(statement);\r\n      console.error(dataToExport);\r\n    } catch (error) {\r\n      console.error(error);\r\n    }\r\n\r\n    const fileName = 'specswallet Report';\r\n    exportFromJSON({ data: dataToExport, fileName, exportType: toexport[format] });\r\n  };\r\n\r\n  const getStatement = async info => {\r\n    setData({ ...data, ...info });\r\n    try {\r\n      const getWalletAccount = await Axios.get(\r\n        `/api/account-statement/${info.accountNumber}/${!isEmpty(info.start) ? info.start : info.dateOpened}/${\r\n          !isEmpty(info.end) ? info.end : moment(Date()).format('YYYY-MM-DD')\r\n        }`\r\n      );\r\n      if (getWalletAccount.data.length && getWalletAccount.data.length > 0) {\r\n        const openingBalance =\r\n          getWalletAccount.data[0]['creditDebit'] === 'debit'\r\n            ? getWalletAccount.data[0]['currentBalance'] + getWalletAccount.data[0]['amount']\r\n            : getWalletAccount.data[0]['currentBalance'] - getWalletAccount.data[0]['amount'];\r\n        setOpening(String(openingBalance));\r\n        setClosing(getWalletAccount.data[getWalletAccount.data.length - 1]['currentBalance']);\r\n      }\r\n      setStatement(getWalletAccount.data);\r\n      console.error(getWalletAccount.data);\r\n    } catch (e) {\r\n      console.error(e);\r\n    }\r\n  };\r\n  useEffect(() => {\r\n    getStatement(props.data);\r\n    //  console.error(props.data,  'hello');\r\n  }, [props.data]);\r\n\r\n  return (\r\n    <>\r\n      <div style={{ float: 'right' }}>\r\n        <select onChange={e => setFormat(e.target.value)} style={{ height: '50px', marginRight: '10px' }} className=\"formatprint\">\r\n          <option value=\"pdf\">pdf</option>\r\n          <option value=\"xls\">xls</option>\r\n          <option value=\"csv\">csv</option>\r\n          <option value=\"txt\">text</option>\r\n          <option value=\"json\">json</option>\r\n        </select>\r\n        {format === 'pdf' && (\r\n          <ReactToPrint\r\n            trigger={() => {\r\n              return (\r\n                <button style={{ marginTop: '10px', marginRight: '40px', border: '1px solid #565682', fontSize: '12px' }}>\r\n                  Download or print\r\n                </button>\r\n              );\r\n            }}\r\n            content={() => toPrint.current}\r\n          />\r\n        )}\r\n        {format !== 'pdf' && (\r\n          <button onClick={exportFile} style={{ marginTop: '10px', marginRight: '40px', border: '1px solid #565682', fontSize: '12px' }}>\r\n            Print {format}\r\n          </button>\r\n        )}\r\n      </div>\r\n      <section ref={toPrint}>\r\n        <span className=\"titles\">Customer Wallet Statement</span>\r\n        <div style={{ marginLeft: '54px' }}>\r\n          <p className=\"titles2\">Summary</p>\r\n          <div className=\"dateContainer\">\r\n            <table>\r\n              <thead></thead>\r\n              <tbody>\r\n                <tr className=\"drow\">\r\n                  <th>Wallet Owner Name: </th>\r\n                  <td>{data.accountOwnerName}</td>\r\n                </tr>\r\n                {/* <tr className = 'drow'>\r\n                               <th>Data Created: </th>\r\n                               <td>{data.dateOpened}</td>\r\n                           </tr> */}\r\n                <tr>\r\n                  <th>Date Range: </th>\r\n                  <td>\r\n                    <b>{!isEmpty(data.start) ? moment(data.start).format('ll') : moment(data.dateOpened).format('ll')}</b> -{' '}\r\n                    <b>{!isEmpty(data.end) ? moment(data.end).format('ll') : moment(Date()).format('ll')}</b>\r\n                  </td>\r\n                </tr>\r\n                <tr className=\"drow\">\r\n                  <th>Wallet Number: </th>\r\n                  <td>{data.accountNumber}</td>\r\n                </tr>\r\n                {!isEmpty(data.schemeName) && (\r\n                  <tr className=\"drow\">\r\n                    <th>Scheme Name: </th>\r\n                    <td>{data.schemeName}</td>\r\n                  </tr>\r\n                )}\r\n                <tr className=\"drow\">\r\n                  <th>Opening Balance: </th>\r\n                  <td>\r\n                    <MoneyFormat\r\n                      value={opening}\r\n                      displayType=\"text\"\r\n                      thousandSeparator={true}\r\n                      decimalScale={2}\r\n                      fixedDecimalScale={true}\r\n                      prefix=\"₦\"\r\n                    />\r\n                  </td>\r\n                </tr>\r\n                <tr className=\"drow\">\r\n                  <th>Closing Balance: </th>\r\n                  <td>\r\n                    <MoneyFormat\r\n                      value={closing}\r\n                      displayType=\"text\"\r\n                      thousandSeparator={true}\r\n                      decimalScale={2}\r\n                      fixedDecimalScale={true}\r\n                      prefix=\"₦\"\r\n                    />\r\n                  </td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n        <div style={{ marginLeft: '54px', marginTop: '50px' }}>\r\n          <div>\r\n            <p className=\"titles2\">REPORT DETAILS</p>\r\n          </div>\r\n          <Table hover className=\"transaction-table\">\r\n            <thead>\r\n              <th>S/N</th>\r\n              <th>Transaction Date</th>\r\n              <th>Reference Number</th>\r\n              <th>External Reference</th>\r\n              <th>Narration</th>\r\n              <th>Debit (₦)</th>\r\n              <th>Credit (₦)</th>\r\n              <th>Balance (₦)</th>\r\n            </thead>\r\n            <tbody>\r\n              <tr>\r\n                <td></td>\r\n                <td></td>\r\n                <td></td>\r\n                <td></td>\r\n                <td style={{ fontSize: '14px' }}>Opening Balance</td>\r\n                <td></td>\r\n                <td></td>\r\n                <td style={{ fontSize: '13px' }}>\r\n                  {\r\n                    <MoneyFormat\r\n                      value={opening}\r\n                      displayType=\"text\"\r\n                      thousandSeparator={true}\r\n                      decimalScale={2}\r\n                      fixedDecimalScale={true}\r\n                      //   prefix = \"₦\"\r\n                    />\r\n                  }\r\n                </td>\r\n              </tr>\r\n              {statement.map((v, i) => (\r\n                <tr className=\"tdata\" key={i}>\r\n                  <td>{i + 1}</td>\r\n                  <td>{moment(v.transactionDate).format('ll')}</td>\r\n                  <td>{v.transactionRef}</td>\r\n                  <td>{v.externalRef !== 'null' ? v.externalRef : ''}</td>\r\n                  <td>{v.memo}</td>\r\n                  <td>\r\n                    <MoneyFormat\r\n                      value={Number(v.debit)}\r\n                      displayType=\"text\"\r\n                      thousandSeparator={true}\r\n                      decimalScale={2}\r\n                      fixedDecimalScale={true}\r\n                      // prefix = \"₦\"\r\n                    />\r\n                  </td>\r\n                  <td>\r\n                    <MoneyFormat\r\n                      value={Number(v.credit)}\r\n                      displayType=\"text\"\r\n                      thousandSeparator={true}\r\n                      decimalScale={2}\r\n                      fixedDecimalScale={true}\r\n                      // prefix = \"₦\"\r\n                    />\r\n                  </td>\r\n                  <td>\r\n                    <MoneyFormat\r\n                      value={Number(v.currentBalance)}\r\n                      displayType=\"text\"\r\n                      thousandSeparator={true}\r\n                      decimalScale={2}\r\n                      fixedDecimalScale={true}\r\n                      // prefix = \"₦\"\r\n                    />\r\n                  </td>\r\n                </tr>\r\n              ))}\r\n              <tr>\r\n                <td></td>\r\n                <td></td>\r\n                <td></td>\r\n                <td></td>\r\n                <td style={{ fontSize: '14px' }}>Closing Balance</td>\r\n                <td></td>\r\n                <td></td>\r\n                <td style={{ fontSize: '13px' }}>\r\n                  {\r\n                    <MoneyFormat\r\n                      value={closing}\r\n                      displayType=\"text\"\r\n                      thousandSeparator={true}\r\n                      decimalScale={2}\r\n                      fixedDecimalScale={true}\r\n                      //   prefix = \"₦\"\r\n                    />\r\n                  }\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </Table>\r\n        </div>\r\n      </section>\r\n    </>\r\n  );\r\n}\r\n\r\nconst mapStateToProps = ({ reports }: IRootState) => ({\r\n  data: reports.data,\r\n});\r\n\r\nconst mapDispatchToProps = { setWalletData };\r\n\r\ntype StateProps = ReturnType<typeof mapStateToProps>;\r\ntype DispatchProps = typeof mapDispatchToProps;\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Wallet);\r\n"]}]}