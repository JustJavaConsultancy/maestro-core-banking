B,OAAO,OAAC,MAAM,CAAC,IAAI,0CAAE,IAAI,CAAC,CAAC;wBAC7B,CAAC,CAAA,eAGM,CACV;oBAEA,IAAI,CAAC,MAAM,GAAG,CAAC,CAAC,CAAC,CAAC,CACjB,oBAAC,OAAO,IAAC,IAAI,EAAE,IAAI,EAAE,OAAO,EAAE,KAAK,CAAC,OAAO,EAAE,QAAQ,EAAE,WAAW,KAAK,CAAC,KAAK,MAAM,EAAE,GAAG,EAAE,WAAW,GAAI,CAC1G,CAAC,CAAC,CAAC,IAAI,CACJ,CACF,CACP;YACA,KAAK,CAAC,QAAQ;YAEd,CAAC,KAAK,CAAC,IAAI,IAAI,CACd,6BAAK,SAAS,EAAC,UAAU;gBACvB,6BAAK,KAAK,EAAE,EAAE,KAAK,EAAE,OAAO,EAAE,QAAQ,EAAE,QAAQ,EAAE;oBAChD,6BAAK,KAAK,EAAE,EAAE,OAAO,EAAE,MAAM,EAAE,IAC5B,IAAI,CAAC,GAAG,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,EAAE,CAAC,CAClB,gCACE,KAAK,EAAE,EAAE,eAAe,EAAE,MAAM,KAAK,CAAC,CAAC,CAAC,CAAC,SAAS,CAAC,CAAC,CAAC,MAAM,EAAE,KAAK,EAAE,MAAM,KAAK,CAAC,CAAC,CAAC,CAAC,MAAM,CAAC,CAAC,CAAC,SAAS,EAAE,EACvG,OAAO,EAAE,GAAG,EAAE;4BACZ,SAAS,CAAC,CAAC,CAAC,CAAC;4BACb,KAAK,CAAC,QAAQ,CAAC,CAAC,CAAC,CAAC;wBACpB,CAAC,EACD,GAAG,EAAE,CAAC,IAEL,CAAC,CACK,CACV,CAAC,CACE,CACF,CACF,CACP,CACG,CACE,CACX,CAAC;AACJ,CAAC","sourcesContent":["import './Layout.scss';\r\nimport React, { useState, useEffect, useRef } from 'react';\r\nimport { faHome, faArrowAltCircleLeft } from '@fortawesome/free-solid-svg-icons';\r\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\r\nimport { useHistory } from 'react-router-dom';\r\nimport ReactToPrint from 'react-to-print';\r\nimport exportFromJSON from 'export-from-json';\r\nimport { isEmpty } from 'lodash';\r\n\r\nimport { CSVLink } from 'react-csv';\r\n\r\nexport default function (props) {\r\n  const [page, setPage] = useState([]);\r\n  const [active, setActive] = useState(0);\r\n  const history = useHistory();\r\n  const [format, setFormat] = useState('pdf');\r\n  const [data, setData] = useState([]);\r\n  const csvInstance = useRef<any | null>(null);\r\n\r\n  const isTrue = history.location.pathname === '/dowmload-wallets' ? true : false;\r\n\r\n  useEffect(() => {\r\n    const pages = Number(props.page) / Number(props.size);\r\n    const p = [];\r\n    for (let x = 0; x < pages; x++) {\r\n      p[x] = x;\r\n    }\r\n    console.error(p);\r\n    setPage(p);\r\n  }, [props.page]);\r\n\r\n  useEffect(() => {\r\n    if (data && csvInstance && csvInstance.current && csvInstance.current.link) {\r\n      setTimeout(() => {\r\n        csvInstance.current.link.click();\r\n        setData([]);\r\n      });\r\n    }\r\n  });\r\n\r\n  const exportFile = () => {\r\n    enum toexport {\r\n      csv = 'csv',\r\n      xls = 'csv',\r\n      txt = 'txt',\r\n      json = 'json',\r\n    }\r\n    const dataToExport = props.toExport;\r\n    const fileName = 'specswallet Report';\r\n    exportFromJSON({ data: dataToExport, fileName, exportType: toexport[format] });\r\n  };\r\n\r\n  return (\r\n    <section className=\"inner-layout-container\">\r\n      <div style={{ display: 'flex', alignItems: 'center' }}>\r\n        <FontAwesomeIcon icon={faHome} />\r\n        <div style={{ marginLeft: '5px' }}>\r\n          <span className=\"path\">{props.path}</span>\r\n        </div>\r\n      </div>\r\n      <div style={{ display: 'flex', alignItems: 'center', marginTop: '30px' }}>\r\n        <FontAwesomeIcon className=\"click\" onClick={() => (props.goBack ? props.goBack() : history.goBack())} icon={faArrowAltCircleLeft} />\r\n        <div style={{ marginLeft: '5px' }}>\r\n          <span style={{ fontSize: '24px' }} className=\"\">\r\n            {props.title}\r\n          </span>\r\n        </div>\r\n      </div>\r\n      <div style={{ backgroundColor: '#fff', marginTop: '20px', borderRadius: '20px', padding: '20px' }}>\r\n        {!props.dontfilter && (\r\n          <div className=\"childToolBar\">\r\n            {!isTrue && (\r\n              <div style={{ flex: 0.5 }}>\r\n                <input onChange={e => props.setSearch(e.target.value)} placeholder=\"Search\" className=\"search-input\" />\r\n              </div>\r\n            )}\r\n            <div style={{ flex: 0.5, display: 'flex', justifyContent: 'flex-end' }}>\r\n              {!isTrue && (\r\n                <select onChange={e => props.schemeChange(e.target.value)} className=\"filter\">\r\n                  {!isEmpty(props.schemes) &&\r\n                    props.schemes.map((s, i) => (\r\n                      <option key={i} value={JSON.stringify(s)}>\r\n                        {s.scheme}\r\n                      </option>\r\n                    ))}\r\n                </select>\r\n              )}\r\n              {/* <div className = 'filter'>\r\n                            <img src = \"content/images/filter.png\"/>\r\n                            <span style = {{marginLeft: '5px', marginRight: '5px'}}>  Filter </span>\r\n                        </div> */}\r\n              <select onChange={e => setFormat(e.target.value)} style={{ height: '50px', marginRight: '10px' }} className=\"filter print\">\r\n                <option value=\"pdf\">pdf</option>\r\n                <option value=\"xls\">xls</option>\r\n                <option value=\"csv\">csv</option>\r\n                <option value=\"txt\">text</option>\r\n                <option value=\"json\">json</option>\r\n              </select>\r\n              {format === 'pdf' ? (\r\n                <ReactToPrint\r\n                  trigger={() => {\r\n                    return (\r\n                      <button className=\"filter print\">{history.location.pathname === '/dowmload-wallets' ? 'Download' : 'Export'}</button>\r\n                    );\r\n                  }}\r\n                  content={() => props.toPrint.current}\r\n                />\r\n              ) : (\r\n                <button onClick={exportFile} className=\"filter\">\r\n                  {history.location.pathname === '/dowmload-wallets' ? 'Download' : 'Export'}\r\n                </button>\r\n              )}\r\n\r\n              {props.headers && (\r\n                <button\r\n                  className=\"download-wallet-btn\"\r\n                  disabled={props.isDisable}\r\n                  onClick={async () => {\r\n                    const result = await props.getData();\r\n                    // eslint-disable-next-line no-console\r\n                    console.log(result);\r\n                    setData(result.data?.data);\r\n                  }}\r\n                >\r\n                  Download\r\n                </button>\r\n              )}\r\n\r\n              {data.length > 0 ? (\r\n                <CSVLink data={data} headers={props.headers} filename={`Pouchii ${props.title}.csv`} ref={csvInstance} />\r\n              ) : null}\r\n            </div>\r\n          </div>\r\n        )}\r\n        {props.children}\r\n\r\n        {!props.show && (\r\n          <div className=\"paginate\">\r\n            <div style={{ width: '250px', overflow: 'scroll' }}>\r\n              <div style={{ display: 'flex' }}>\r\n                {page.map((p, i) => (\r\n                  <button\r\n                    style={{ backgroundColor: active === i ? '#565682' : '#fff', color: active === i ? '#fff' : '#565682' }}\r\n                    onClick={() => {\r\n                      setActive(i);\r\n                      props.nextPage(i);\r\n                    }}\r\n                    key={i}\r\n                  >\r\n                    {i}\r\n                  </button>\r\n                ))}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        )}\r\n      </div>\r\n    </section>\r\n  );\r\n}\r\n"]}]}