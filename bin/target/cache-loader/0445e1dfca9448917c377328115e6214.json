,EAAC,QAAQ,oBAEzC;4BACR,+BACE,KAAK,EAAE,KAAK,EACZ,SAAS,EAAC,eAAe,EACzB,IAAI,EAAC,OAAO,EACZ,EAAE,EAAC,QAAQ,EACX,IAAI,EAAC,QAAQ,EACb,WAAW,EAAC,eAAe,EAC3B,QAAQ,EAAE,YAAY,GACtB,CACE;wBAEN,6BAAK,SAAS,EAAC,oBAAoB;4BACjC,qDAA2B;4BAE3B,6BAAK,SAAS,EAAC,qBAAqB;gCAClC,6BAAK,SAAS,EAAC,UAAU;oCACvB,+BAAO,SAAS,EAAC,eAAe,EAAC,OAAO,EAAC,MAAM,YAEvC;oCAER,+BAAO,KAAK,EAAE,SAAS,EAAE,SAAS,EAAC,eAAe,EAAC,IAAI,EAAC,WAAW,EAAC,EAAE,EAAC,MAAM,EAAC,IAAI,EAAC,MAAM,EAAC,QAAQ,EAAE,YAAY,GAAI,CAChH;gCAEN;oCACE,+BAAO,SAAS,EAAC,eAAe,EAAC,OAAO,EAAC,IAAI,UAErC;oCACR,+BAAO,KAAK,EAAE,OAAO,EAAE,SAAS,EAAC,eAAe,EAAC,IAAI,EAAC,SAAS,EAAC,EAAE,EAAC,IAAI,EAAC,IAAI,EAAC,MAAM,EAAC,QAAQ,EAAE,YAAY,GAAI,CAC1G,CACF,CACF;wBAEL,GAAG,IAAI,CACN,6BAAK,SAAS,EAAC,aAAa;4BAC1B,+BAAI,GAAG,CAAK,CACR,CACP;wBAED,6BAAK,SAAS,EAAC,iBAAiB;4BAC9B,oCAAS,CAAC,SAAS,CAAC,CAAC,CAAC,aAAa,CAAC,CAAC,CAAC,8BAAM,SAAS,EAAC,cAAc,GAAQ,CAAU,CAClF,CACD,CACH,CACL,CACS,CACP,CACV,CAAC;AACJ,CAAC","sourcesContent":["import React, { ChangeEvent, useEffect, useState, useRef } from 'react';\r\nimport Layout from '../dashboard/Layout';\r\nimport InnerLayout from '../dashboard/components/Layout';\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport { useHistory } from 'react-router';\r\n\r\nimport './reportCashConnect.scss';\r\n\r\nexport default function () {\r\n  const history = useHistory();\r\n\r\n  const tableRef = useRef<HTMLDivElement>(null);\r\n  const [data, setData] = useState({\r\n    nuban: '',\r\n    startDate: '',\r\n    endDate: '',\r\n  });\r\n  const [err, setErr] = useState('');\r\n  const [isLoading, setIsLoading] = useState(false);\r\n\r\n  const { endDate, nuban, startDate } = data;\r\n\r\n  const handleChange = (e: ChangeEvent<HTMLInputElement>) => {\r\n    setErr('');\r\n\r\n    const { name, value } = e.target;\r\n    setData({ ...data, [name]: value });\r\n  };\r\n\r\n  const handleSubmit = async (e: ChangeEvent<HTMLFormElement>) => {\r\n    e.preventDefault();\r\n    setIsLoading(true);\r\n\r\n    if (!(nuban.length > 9 && Number(nuban))) {\r\n      setErr('Invalid nuban account');\r\n      setIsLoading(false);\r\n      return;\r\n    }\r\n\r\n    const fromDate = startDate ? moment().format(startDate) : moment().format('YYYY-MM-DD');\r\n    const toDate = endDate ? moment().format(endDate) : moment().format('YYYY-MM-DD');\r\n\r\n    try {\r\n      const response = await axios.get(`/api/cash-connect/account/statement?accountNumber=${nuban}&fromDate=${fromDate}&toDate=${toDate}`);\r\n      // const response = await axios.get(\r\n      //   `https://walletdemo.remita.net/api/cash-connect/account/statement?accountNumber=${nuban}&fromDate=${fromDate}&toDate=${toDate}`\r\n      // );\r\n      // eslint-disable-next-line no-console\r\n      console.log(response);\r\n      if (response.data.code === '00') {\r\n        const jsonResponse = JSON.parse(response.data.data);\r\n\r\n        history.push({ pathname: 'cashconnect-reports-details', state: { jsonResponse, fromDate, toDate } });\r\n      }\r\n    } catch (error) {\r\n      setErr('Failed to fetch report!');\r\n      setIsLoading(false);\r\n    }\r\n\r\n    // // eslint-disable-next-line no-console\r\n    // console.log(tableRef.current.getBoundingClientRect().top);\r\n\r\n    // const tablePosition = tableRef.current.getBoundingClientRect().top;\r\n\r\n    // window.scrollTo({ behavior: 'smooth', top: tablePosition });\r\n  };\r\n\r\n  return (\r\n    <Layout>\r\n      <InnerLayout dontfilter={true} show={true} title=\"CashConnect Report\" path=\"cashconnect-report\">\r\n        <>\r\n          <div className=\"connect-box\">\r\n            <div className=\"connect-box-header\">\r\n              <h5>Please fill the form below to view report </h5>\r\n            </div>\r\n\r\n            <form className=\"connect-form\" onSubmit={handleSubmit}>\r\n              <div className=\"connect-wallet\">\r\n                <label className=\"connect-label\" htmlFor=\"wallet\">\r\n                  Nuban wallet:\r\n                </label>\r\n                <input\r\n                  value={nuban}\r\n                  className=\"connect-input\"\r\n                  name=\"nuban\"\r\n                  id=\"wallet\"\r\n                  type=\"number\"\r\n                  placeholder=\"nuban account\"\r\n                  onChange={handleChange}\r\n                />\r\n              </div>\r\n\r\n              <div className=\"connect-filter-box\">\r\n                <h6>Filter report date</h6>\r\n\r\n                <div className=\"connect-filter-date\">\r\n                  <div className=\"date-box\">\r\n                    <label className=\"connect-label\" htmlFor=\"from\">\r\n                      From:\r\n                    </label>\r\n\r\n                    <input value={startDate} className=\"connect-input\" name=\"startDate\" id=\"from\" type=\"date\" onChange={handleChange} />\r\n                  </div>\r\n\r\n                  <div>\r\n                    <label className=\"connect-label\" htmlFor=\"to\">\r\n                      To:\r\n                    </label>\r\n                    <input value={endDate} className=\"connect-input\" name=\"endDate\" id=\"to\" type=\"date\" onChange={handleChange} />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              {err && (\r\n                <div className=\"connect-err\">\r\n                  <p>{err}</p>\r\n                </div>\r\n              )}\r\n\r\n              <div className=\"connect-btn-box\">\r\n                <button>{!isLoading ? 'View Report' : <span className=\"connect-spin\"></span>}</button>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </>\r\n      </InnerLayout>\r\n    </Layout>\r\n  );\r\n}\r\n"]}]}