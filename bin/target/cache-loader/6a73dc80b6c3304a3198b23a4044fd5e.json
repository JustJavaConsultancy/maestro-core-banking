from 'moment';\r\n\r\nimport './reportDetail.scss';\r\n\r\nexport default function () {\r\n  const location = useLocation();\r\n\r\n  const [data, setData] = useState({\r\n    startDate: location.state.fromDate,\r\n    endDate: location.state.toDate,\r\n    transactions: location.state.jsonResponse.Message,\r\n  });\r\n\r\n  const { endDate, startDate, transactions } = data;\r\n\r\n  // eslint-disable-next-line no-console\r\n  console.log(location);\r\n\r\n  return (\r\n    <section id=\"connect-details\">\r\n      <div className=\"detail-header\">\r\n        <h3>CashConnect Transaction Report</h3>\r\n        <p>\r\n          <span>{moment(startDate).format('LL')}</span> - <span>{moment(endDate).format('LL')}</span>\r\n        </p>\r\n      </div>\r\n\r\n      <div>\r\n        {transactions.length < 1 ? (\r\n          <div style={{ textAlign: 'center' }}>\r\n            <Table style={{ marginBottom: '10rem' }}>\r\n              <thead>\r\n                <tr>\r\n                  <th>S/N</th>\r\n                  <th>ID</th>\r\n                  <th>Amount</th>\r\n                  <th>Credit</th>\r\n                  <th>Debit</th>\r\n                  <th>Opening Balance</th>\r\n                  {/* <th>InstrumentNo</th> */}\r\n                  <th>Record Type</th>\r\n                  <th>Reference ID</th>\r\n                  {/* <th>Service Code</th> */}\r\n                  <th>Reversed</th>\r\n                  <th>Narration</th>\r\n                  <th>Withdrawable Amount</th>\r\n                  <th>Transaction Date</th>\r\n                </tr>\r\n              </thead>\r\n            </Table>\r\n            <h1>No Data</h1>\r\n          </div>\r\n        ) : (\r\n          <Table>\r\n            <thead>\r\n              <tr>\r\n                <th>S/N</th>\r\n                <th>ID</th>\r\n                <th>Amount</th>\r\n                <th>Credit</th>\r\n                <th>Debit</th>\r\n                <th>Opening Balance</th>\r\n                {/* <th>InstrumentNo</th> */}\r\n                <th>Record Type</th>\r\n                <th>Reference ID</th>\r\n                {/* <th>Service Code</th> */}\r\n                <th>Reversed</th>\r\n                <th>Narration</th>\r\n                <th>Withdrawable Amount</th>\r\n                <th>Transaction Date</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              {transactions.map((transaction, i) => (\r\n                <tr key={i}>\r\n                  <td>{i + 1}.</td>\r\n                  <td>{transaction.Id}</td>\r\n                  <td>{transaction.Amount}</td>\r\n                  <td>{transaction.Credit}</td>\r\n                  <td>{transaction.Debit}</td>\r\n                  <td>{transaction.OpeningBalance}</td>\r\n                  <td>{transaction.RecordType}</td>\r\n                  <td>{transaction.ReferenceID}</td>\r\n                  <td>{transaction.IsReversed ? 'true' : 'false'}</td>\r\n                  <td>{transaction.Narration}</td>\r\n                  <td>{transaction.WithdrawableAmount}</td>\r\n                  <td>{moment(transaction.TransactionDate).format('MMM Do YY')}</td>\r\n                </tr>\r\n              ))}\r\n            </tbody>\r\n          </Table>\r\n        )}\r\n      </div>\r\n    </section>\r\n  );\r\n}\r\n"]}]}